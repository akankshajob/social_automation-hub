{
  "name": "social-automation-hub",
  "version": "1.0.0",
  "description": "A powerful full-stack social media scheduling and automation platform built with React, Express.js, and n8n integration",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "social-media",
    "scheduler",
    "n8n",
    "automation"
  ],
  "author": "Your Name",
  "license": "MIT"
}
